     #"beta.dat" u 1:2 smooth bezier w l lt 2 lw 1 lc rgb "brown" title ""

#plot "beta.dat" u 1:2 title "" smooth bezier w l lt 1 lw 6 lc rgb "gray"

unset multiplot


